<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <appSettings>
    <add key="scraperUrlBase" value="https://www.healcode.com/widgets/schedules/print/{0}?"/>
    <add key="scraperUrlQueryString" value="options[location]={0}&amp;options[start_date]={1}&amp;options[end_date]={2}"/>
    <add key="scraperUrlShortDateFormat" value="yyyy-MM-dd"/>
    <add key="scraperCacheDurationInMinutes" value="720"/>
  </appSettings>
</configuration>